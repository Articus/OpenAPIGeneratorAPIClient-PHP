{
    "name": "articus/openapi-generator-apiclient",
    "description": "Common classes for php-dt generator of OpenAPI Generator",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "articus/data-transfer": "^0.6",
        "articus/plugin-manager": "^0.1.1",
        "psr/container": "^1.0|^2.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.1|^2.0"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\": "spec/"
        }
    },
    "require-dev": {
        "ext-json": "*",
        "ext-uopz": "*",
        "kahlan/kahlan": "^5.1",
        "mockery/mockery": "^1.3",
        "symfony/polyfill-mbstring": "^1.27"
    }
}
